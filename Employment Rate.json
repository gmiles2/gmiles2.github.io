{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Google's stock price over time.",
  "title": {
    "text": "UK Employment Rate (%)",
    "fontSize": 12,
    "anchor": "start",
    "subtitle": "Employment rate in the UK for aged 16-64, SA",
    "subtitleFontSize": 8,
    "color": "black"
  },
  "width": 150,
  "height": 150,
  "data": {
    "url": "https://raw.githubusercontent.com/gmiles2/gmiles2.github.io/main/Data_ONS-LF24.json",
    "format": {"type": "json", "property": "months"}
  },
  "mark": {
    "type": "line",
    "interpolate": "basis",
    "color": "green",
    "strokeWidth": 1,
    "opacity": 1,
    "clip": true
  },
  "encoding": {
    "x": {
      "field": "year",
      "type": "temporal",
      "title": "Year",
      "scale": {"domain": [{"year": 1971}, {"year": 2022}]}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Employment Rate (%)"
    }
  },
  "config": {}
}